//2.Count Digits : Ex: N =7789 OUTPUT: 4
//code360: https://bit.ly/3X17nIr
//GFG : https://www.geeksforgeeks.org/problems/count-digits5716/1
package Maths;

public class Count_Digits {
	public static void main(String[] args) {
		int n = 123, lasDigit,count=0;
		while(n>0) {
			lasDigit= n%10;
			n = n/10;
			count= count+1;
		}
		System.out.println("output = "+count); // output = 3
	}
}
//TC = O(log n)
//SC = O(1)
