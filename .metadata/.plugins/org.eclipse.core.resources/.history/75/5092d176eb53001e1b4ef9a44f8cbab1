package collection;
import java.util.HashMap;
import java.util.Map;
public class map_Collection {

	public static void main(String[] args) {
		
		Map m = new HashMap();
		m.put(101 , "Shu");
		m.put(102 , "kamal");
		m.put(103 , "Rahul");
		m.put(104 , "Rohan");
		System.out.println(m); // Output-> {101=Shu, 102=kamal, 103=Rahul, 104=Rohan}

	}

}
