//Reverse the array using recursion(Two Pointer).

package Recursion;

public class Reverse_the_Array {

	//Print the array
	static void printArray(int arr[], int i,int n) {
		for(i=1;i<=n;i++) {
			System.out.print(arr[i] +" ");
		}
	}
	public static void main(String[] args) {
		int arr[]= {1,2,3,4,5,8,9,11};
		printArray(arr,1,8);
	}

}
